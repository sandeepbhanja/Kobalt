{
  "name": "shop",
  "version": "1.0.0",
  "description": "E-commerce website using MERN stack",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node backend/index.js",
    "server": "nodemon backend/index.js --ignore client",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "author": "Sandeep Bhanja",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sandeepbhanja/Kobalt/issues"
  },
  "homepage": "https://github.com/sandeepbhanja/Kobalt#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "concurrently": "^8.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.3.1",
    "nodemon": "^2.0.22"
  },
  "devDependencies": {
    "dotenv": "^16.3.1"
  }
}
